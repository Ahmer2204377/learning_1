function add(vhs){
    const length= vhs.length;
    var sum=0;
 for(let i=0; i < length ; i++ ){
    
    sum= sum+vhs[i].weight;
 }
return sum;

}





var cars=[{
   tyre:4,
   engine:3000,
   door : 4,
   length : 5,
   weight : 1000,
   area : 0
}
   ,{
      tyre:4,
      engine:3000,
      door : 4,
      length : 5,
      weight : 1000,
      area : 0
   }]

   
add(cars)